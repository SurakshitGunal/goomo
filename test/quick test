//1. Write a function to list down the keys present in a javascript object
//example object

<script>
var person = {
  firstName: "Goomo",
  lastName: "Test",
  salary: 5000,
  empo: "g1211"
};
function key(a){
console.log(Object.keys(a));
};
key (person);

</script>
//2. Write a function to print the total number of keys present in a javascript object
//example object



<script>
var object1 = {
    fname: "Goomo",
    lname: "Test",
    salary: 1000,
    empId: "G1121",
};
function count(a){
console.log(Object.keys(a).length);    
}
count(object1);
</script>


